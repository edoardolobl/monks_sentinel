{
  "name": "@monks-sentinel/gtm-{MODULE_NAME}-module",
  "version": "1.0.0",
  "description": "GTM {MODULE_NAME.title()} Analysis Module - gRPC TypeScript Implementation",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "build": "npm run proto:generate && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "proto:generate": "npm run proto:clean && npm run proto:compile",
    "proto:clean": "rimraf generated/*",
    "proto:compile": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./generated --grpc_out=grpc_js:./generated --js_out=import_style=commonjs:./generated -I ../../proto ../../proto/*.proto",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "gtm",
    "google-tag-manager",
    "{MODULE_NAME}",
    "grpc",
    "typescript",
    "quality-assurance",
    "analysis"
  ],
  "author": "Monks Sentinel",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.6",
    "@grpc/proto-loader": "^0.7.10",
    "google-protobuf": "^3.21.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist/**/*",
    "generated/**/*",
    "README.md"
  ]
}